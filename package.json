{
  "name": "tailwindcss-parcel-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hacknug/tailwindcss-parcel-boilerplate.git"
  },
  "scripts": {
    "postcss:watch": "postcss src/index.pcss -o src/index.css --watch",
    "parcel:serve": "parcel src/index.html",
    "parcel:watch": "parcel watch src/index.html",
    "parcel:build": "parcel build src/index.html",
    "start": "rm -rf dist .cache && run-p postcss:watch parcel:serve"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind css",
    "parcel",
    "postcss"
  ],
  "author": "Nestor Vera <nestorvera@me.com> (https://nestor.rip)",
  "license": "ISC",
  "dependencies": {
    "parcel-bundler": "^1.9.7"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0",
    "tailwindcss": "^0.6.4"
  }
}
